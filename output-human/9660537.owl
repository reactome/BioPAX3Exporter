 <bp:evidence rdf:resource="#Evidence2" />
 <bp:displayName rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">Gain-of-function MRAS complexes activate RAF signaling</bp:displayName>
 <bp:comment rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">This pathway describes the effect of activating mutations of MRAS-complex components on RAF activation (reivewed in Simanshu et al, 2017).</bp:comment>
 <bp:dataSource rdf:resource="#Provenance1" />
</bp:Pathway>

<bp:Pathway rdf:ID="Pathway1">
