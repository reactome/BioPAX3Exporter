 <bp:displayName rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">Deletions of the AMER1 gene prevent expression of AMER1 protein</bp:displayName>
</bp:BiochemicalReaction>

<bp:Pathway rdf:ID="Pathway2">
 <bp:pathwayOrder rdf:resource="#PathwayStep2" />
